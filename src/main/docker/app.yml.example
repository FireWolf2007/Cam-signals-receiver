version: '2'
services:
    cam-signals-receiver-app:
        image: cam-signals-receiver
        environment:
            - SPRING_PROFILES_ACTIVE=prod
        ports:
            - 8089:8089
    networks:
      app_net:
        ipv6_address: 2001:3200:3200::20
networks:
  app_net:
    enable_ipv6: true
    driver: bridge
    ipam:
      driver: default
      config:
        -- subnet: 2001:3200:3200::/64
          gateway: 2001:3200:3200::1